          <div class="twelve wide column">
            <div class="ui container">
              
              
              <% jobs.forEach(function(job){ %>
              
              <div style="width: 100%" class="ui card jobs">
                <div class="content">
                  <div class="ui internally celled two column grid">
                  <div style="padding: 0" class="five wide column container">
                     <img style="width: 100%; height: 100%" src="<%= job.image %>">
                  </div>

                  <div class="nine wide column">
                  <h3 class="ui blue left aligned header title"><%= job.title %></h3>
                  <h3 class="ui left aligned sub header"><i class="blue marker outline icon"></i><%= job.location %></h3>
                  <div class="ui small feed">
                    <div class="event">
                      <div class="content">
                        <div class="summary">
                           <i style="padding: 0px 1em 0px 0px" class=" calendar outline icon"></i><%= job.date.toDateString() %>  
                           <i style="padding: 0px 1em 0px 1em" class="money bill alternate outline icon"></i> <%= job.min_sal %> - <%= job.max_sal %> 
                           <i style="padding: 0px 1em 0px 1em" class="file outline icon"></i><%= job.experience %>
                        </div>
                      </div>
                    </div>
                    <div class="event">
                      <div class="content">
                        <div class="summary">
                          <div class="meta"><%= job.desc.substring(0,100) %> [..] </div>
                        </div>
                      </div>
                    </div>
                   
                  </div>
                  <div class="left aligned extra content">
                  <!--<button class="ui black button">Join Project</button>-->
                  <a href="/detail/<%= job._id %>" class="ui black button">View More</a>
                </div>
                  </div>

                </div>
                </div>
              </div>
              
               <% }) %>
              
